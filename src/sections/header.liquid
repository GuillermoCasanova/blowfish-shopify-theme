



<!--Navigation-->
<nav data-navigation
     data-section-id="{{ section.id }}" data-section-type="header"  data-template="{{ template.name }}" 
     data-ui-component="navigation"
     class="fixed-nav"
     sticky-nav>


    <!--Free Shipping Header--> 
    {%- if section.settings.announcement_bar_enabled -%}
    <alert class="header-alert">
      <div class="container-fluid">
        <div class="row">
          <div class="col-sm-12">
             <div class="header-alert__inner">
              {%- if section.settings.announcement_bar_link != blank -%}
                 <a href="{{ section.settings.announcement_bar_link }}" rel="internal" title="{{section.settings.announcement_bar_link_title}}">
                   {{ section.settings.announcement_bar_text | escape }}
                 </a>
               {%- else -%}
                 <span>{{ section.settings.announcement_bar_text | escape }}</span>
               {%- endif -%}
             </div>
          </div>
        </div>
      </div>
    </alert>
    {%- endif -%}
    <!-- End Free Shipping Header--> 

  <!-- Navigation for devices--> 
  <div class="main-nav" data-main-navigation>
    <div class="main-nav__inner">
      
      <button aria-label="Open off canvas navigation" data-menu-toggle class="main-nav__menu-toggle">
        <div class="main-nav__menu-toggle__icon">
           
        </div>
      </button >

      {% if template.name == 'index' %}
      <h1 class="main-nav__title">
      {% else %}
      <div class="main-nav__title">
      {%endif%}
       <a href="/" class="logo-image" rel="internal" title="{{ shop.name }}">
        <span class="visually-hidden">Blowfish For Hangovers</span>
        <img src="{{'logo--blowfish.svg' | asset_url}}" alt="Blowfish Logo" width="260" class="main-nav__title__logo" data-normal-header-logo> 
        <img src="{{'logo--blowfish--white.svg' | asset_url}}" alt="Blowfish Logo" width="260" class="main-nav__title__logo  main-nav__title__logo--alt" data-alt-header-logo>
       </a>
      {% if template.name == 'index' %}
        </h1>
      {% else %}
      </div>
      {%endif%}

      <button data-cart-toggle aria-label="Toggle Off Canvas Cart" class="main-nav__cart-toggle  hide-for-medium-up"> 
       <div class="main-nav__cart-toggle__icon  header-cart {%if cart.item_count > 0 %} is-filled {%else%} is-empty{%endif%}" data-cart-icon>
            <img src="{{ 'cart--isFilled.svg' | asset_url}}" alt="Cart Icon Filled with Blowfish Character"  class="icon icon--cart-filled">
            <img src="{{ 'cart--alt--isFilled.svg' | asset_url}}" alt="Cart Icon Filled with Blowfish Character"  class="alt-icon icon--cart-filled">
            <img src="{{ 'cart.svg' | asset_url}}" alt="Cart icon that is empty"  class="icon icon--cart-empty">
            <img src="{{ 'cart--alt.svg' | asset_url}}" alt="Cart icon that is empty" class="alt-icon icon--cart-empty">
        </div>
      </button>

      <div class="main-nav__menu-container"  data-off-canvas-menu>

          <button
             class="main-nav__menu-close"
             aria-label="Close navigation"
             data-menu-toggle>
            <div class="main-nav__menu-close__icon" >
               
            </div>
          </button>

          <div class="hide-for-medium-up">
            <a href="/" rel="internal" title="Blowfish Home" class="main-nav__logo">
              <img src="{{'logo--blowfish--white.svg' | asset_url}}" alt="Blowfish Logo" role="presentation">
            </a>
          </div>

          <ul class="main-nav__menu">
              <li class="hide-for-medium-up">
                <a href="/products/starter-pack-20-tablets" rel="internal" class="main-nav__menu__buy-now  button--small button button--secondary button--expanded">
                    <span class="button-textState1">buy now</span>
                    <span class="button-textState2">buy now</span>
                </a>
              </li>
        
                {% for link in linklists[section.settings.menu].links %}
                  {% if link.links != blank %}
                    <li class="main-nav__menu__item  main-nav--minimal-hiddenElm">
                      <a class="main-nav__menu__link" href="{{ link.url }}">
                        {{ link.title }}
                        {% include 'icon-arrow-down' %}
                      </a>
                      <ul>
                        {% for childlink in link.links %}
                          <li>
                            <a href="{{ childlink.url }}">
                              {{ childlink.title }}
                            </a>
                          </li>
                        {% endfor %}
                      </ul>
                    </li>
                  {% else %}
                    <li class="main-nav__menu__item">
                      <a class="main-nav__menu__link" rel="internal" title="{{link.title}}" href="{{ link.url }}">
                        {{ link.title }}
                      </a>
                    </li>
                  {% endif %}
                {% endfor %}
        

              <li class="main-nav__menu__item  show-for-medium-up">
                
                <button data-cart-toggle aria-label="Toggle Off Canvas Cart" class="main-nav__cart-toggle  hide-for-medium--up"> 
                 <div class="main-nav__cart-toggle__icon  header-cart {%if cart.item_count > 0 %} is-filled {%else%} is-empty{%endif%}" data-cart-icon>
                      <img src="{{ 'cart--isFilled.svg' | asset_url}}" alt="Cart Icon Filled with Blowfish Character"  class="icon icon--cart-filled">

                      <img src="{{ 'cart--alt--isFilled.svg' | asset_url}}" alt="Cart Icon Filled with Blowfish Character"  class="alt-icon icon--cart-filled">

                      <img src="{{ 'cart.svg' | asset_url}}" alt="Cart icon that is empty"  class="icon icon--cart-empty">
                      <img src="{{ 'cart--alt.svg' | asset_url}}" alt="Cart icon that is empty" class="alt-icon icon--cart-empty">
                  </div>
                </button>
              </li>

              <li class="main-nav__menu__item   show-for-medium-up">
                <div>
                  <a href="/products/starter-pack-20-tablets" rel="internal" class="main-nav__menu__buy-now  button--small button button--secondary button--expanded">
                      <span class="button-textState1">buy now</span>
                      <span class="button-textState2">buy now</span>
                  </a>
                </div>
              </li>
        

              <ul class="main-nav__menu__social">
        
                 <li>
                    <a target="_blank" href="https://www.facebook.com/Blowfish/" class="icon-facebook main-nav__menu__social__link">
                      <svg xmlns="http://www.w3.org/2000/svg"  viewBox="0 0 100 100">Facebook<path d="M72.089.02L59.624 0C45.62 0 36.57 9.285 36.57 23.656v10.907H24.037a1.96 1.96 0 0 0-1.96 1.961v15.803a1.96 1.96 0 0 0 1.96 1.96H36.57v39.876a1.96 1.96 0 0 0 1.96 1.96h16.352a1.96 1.96 0 0 0 1.96-1.96V54.287h14.654a1.96 1.96 0 0 0 1.96-1.96l.006-15.803a1.963 1.963 0 0 0-1.961-1.961H56.842v-9.246c0-4.444 1.059-6.7 6.848-6.7l8.397-.003a1.96 1.96 0 0 0 1.959-1.96V1.98A1.96 1.96 0 0 0 72.089.02z"/></svg>
                    </a>
                  </li>
        
                  <li>
                    <a target="_blank" href="https://www.instagram.com/blowfishangover/" class="main-nav__menu__social__link icon-instagram">
                      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 180 180">Instagram<path d="M122.406 0H46.654C20.929 0 0 20.93 0 46.655v75.752c0 25.726 20.929 46.655 46.654 46.655h75.752c25.727 0 46.656-20.93 46.656-46.655V46.655C169.063 20.93 148.133 0 122.406 0zm31.657 122.407c0 17.455-14.201 31.655-31.656 31.655H46.654C29.2 154.063 15 139.862 15 122.407V46.655C15 29.201 29.2 15 46.654 15h75.752c17.455 0 31.656 14.201 31.656 31.655v75.752z"/><path d="M84.531 40.97c-24.021 0-43.563 19.542-43.563 43.563 0 24.02 19.542 43.561 43.563 43.561s43.563-19.541 43.563-43.561c0-24.021-19.542-43.563-43.563-43.563zm0 72.123c-15.749 0-28.563-12.812-28.563-28.561 0-15.75 12.813-28.563 28.563-28.563s28.563 12.813 28.563 28.563c0 15.749-12.814 28.561-28.563 28.561zM129.921 28.251c-2.89 0-5.729 1.17-7.77 3.22a11.053 11.053 0 0 0-3.23 7.78c0 2.891 1.18 5.73 3.23 7.78 2.04 2.04 4.88 3.22 7.77 3.22 2.9 0 5.73-1.18 7.78-3.22 2.05-2.05 3.22-4.89 3.22-7.78 0-2.9-1.17-5.74-3.22-7.78-2.04-2.05-4.88-3.22-7.78-3.22z"/></svg>
                    </a> 
                  </li>
        
                  <li>
                    <a target="_blank" href="https://twitter.com/blowfishangover?lang=en" class="main-nav__menu__social__link icon-twitter">
                      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 612 612">Twitter<path d="M612 116.258a250.714 250.714 0 0 1-72.088 19.772c25.929-15.527 45.777-40.155 55.184-69.411-24.322 14.379-51.169 24.82-79.775 30.48-22.907-24.437-55.49-39.658-91.63-39.658-69.334 0-125.551 56.217-125.551 125.513 0 9.828 1.109 19.427 3.251 28.606-104.326-5.24-196.835-55.223-258.75-131.174-10.823 18.51-16.98 40.078-16.98 63.101 0 43.559 22.181 81.993 55.835 104.479a125.556 125.556 0 0 1-56.867-15.756v1.568c0 60.806 43.291 111.554 100.693 123.104-10.517 2.83-21.607 4.398-33.08 4.398-8.107 0-15.947-.803-23.634-2.333 15.985 49.907 62.336 86.199 117.253 87.194-42.947 33.654-97.099 53.655-155.916 53.655-10.134 0-20.116-.612-29.944-1.721 55.567 35.681 121.536 56.485 192.438 56.485 230.948 0 357.188-191.291 357.188-357.188l-.421-16.253c24.666-17.593 46.005-39.697 62.794-64.861z" /></svg>
                    </a>
                  </li>
                 </ul>
            </li>
          </ul>

      </div>
    </div>
  </div>
  <!--End small devices navigation--> 


</nav>
<!--End Navigation-->


<!--Cart Container--> 
<aside data-off-canvas-cart class="off-canvas-cart">

  <div class="off-canvas-cart__loader">
    <div class="off-canvas-cart__loader__animation">
      <div></div>
      <div></div>
    </div>
    <span class="off-canvas-cart__loader__text">Updating Cart</span>
  </div>

  <button hidden  class="off-canvas-cart__close" title="Close off canvas cart" data-close-off-canvas-cart>
    <span class="off-canvas-cart__close__icon">

    </span>
  </button>

  <button class="off-canvas-cart__continue-shopping" aria-label="Close cart and continue shopping" data-close-off-canvas-cart>
    <span>
        <span class="off-canvas-cart__continue-shopping__arrow">
        <svg xmlns="http://www.w3.org/2000/svg" id="Layer_1" x="0" y="0" version="1.1" viewBox="0 0 29.8 27" xml:space="preserve" aria-hidden="true" ><title>Arrow pointing left</title><path d="M2.9 7l25.9.1M1 7l8.3 6.1M1 7l8.3-6"  style="fill:none;stroke:#7a7a7a;stroke-width:2;stroke-linecap:round;stroke-miterlimit:10;"/></svg>
      </span>

      <span class="off-canvas-cart__continue-shopping__text">
        Continue Shopping
      </span>
    </span>
    
  </button>

  <div  id="CartContainer"  class="off-canvas-cart__inner">
    
  </div>
</aside>
<!-- END Cart Container--> 


<div class="off-canvas-cart-overlay"  data-off-canvas-cart-overlay>
  
</div>



{% include 'ajax-cart-template' %}
{% include 'ajax-cart-empty' %}


{% schema %}
  {
    "name": "Header",
    "settings": [
      {
        "type": "header",
        "content": "Announcement bar"
      },
      {
        "type": "checkbox",
        "id": "announcement_bar_enabled",
        "label": "Show announcement",
        "default": false
      },
      {
        "type": "checkbox",
        "id": "announcement_bar_home_page_only",
        "label": "Show on home page only",
        "default": true
      },
      {
        "type": "text",
        "id": "announcement_bar_text",
        "label": "Announcement text",
        "default": "Announce something here"
      },
      {
        "type": "text",
        "id": "announcement_bar_link_title",
        "label": "Announcement link title",
        "default": "The title for the announcement link"
      },
      {
        "type": "url",
        "id": "announcement_bar_link",
        "label": "Announcement link",
        "info": "Optional"
      },
      {
        "type": "header",
        "content": "Header"
      },
      {
        "type": "link_list",
        "id": "menu",
        "label": "Menu",
        "default": "main-menu"
      },
      {
        "type": "header",
        "content": "Buy Now Button"
      },
      {
        "type": "url",
        "id": "buy-now-link",
        "label": "Destination Link"
      },
      {
        "type": "text",
        "id": "buy-now-label",
        "label": "Button Label"
      }
    ]
  }
{% endschema %}
