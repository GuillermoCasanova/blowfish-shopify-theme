
<main>
	<section class="landing-promo">
		<div class="landing-promo__inner">
			<div class="container-fluid">
				<div class="row center-lg">
					<div class="col-sm-12 col-lg-10">
						<div class="row">
							<div class="col-sm-12  col-md-6">
								<div class="landing-promo__image">
									<img src="{{block.settings.image | img_url: '800x'}}" alt="{{block.settings.image.alt}}">
								</div>
							</div>
							<div class="col-sm-12  col-md-6">
								<div>
									<div class="landing-promo__logo">
										<img src="{{'logo--blowfish.svg' | asset_url}}" alt="Blowfish Logo">
									</div>
								</div>
								
								<div>
									<h1 class="landing-promo__headline">
										{{block.settings.headline}}
									</h1>
				
									{{block.settings.body}}
						
									<div>
									{{block.settings.email-embed}}
									</div>
								</div>
							
							</div>
					</div>
				</div>
			</div>
		</div>
	</section>

</main>